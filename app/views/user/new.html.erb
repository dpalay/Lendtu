<%= params.pretty_inspect %>
<br /><br />
<%= form_for @user do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %> <br/>
    <%= f.label :email %>
    <%= f.text_field :email %> <br/>
    <%= f.label :first_name %>
    <%= f.text_field :first_name %><br/>
    <%= f.label :last_name %>
    <%= f.text_field :last_name %> <br/>
    <%= f.submit %>
<% end %>
