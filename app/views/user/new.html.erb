<%= form_for :user, url: user_index_path do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %> <br/>
    <%= f.label :email %>
    <%= f.text_field :email %><br/>
    <%= f.label :first_name %>
    <%= f.text_field :first_name %><br/>
    <%= f.label :last_name %>
    <%= f.text_field :last_name %> <br/>
    <%= f.submit %>
<% end %>

<br /><br />
